{% extends 'AppBundle::layout.html.twig' %}

{% block body -%}
    <h1>Qrcode</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ entity.content }}</td>
            </tr>
            <tr>
                <th>Deviceid</th>
                <td>{{ entity.deviceId }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('qrcode') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('qrcode_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
